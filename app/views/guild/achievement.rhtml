<% @title = @achievement.name %>
<%= render :partial => "header" %>

<div id="content">
    <p>Members of <%= @guild.name %> that have achieved:</p>
    <h1><%= h @achievement.name %></h1>
    <p><%= h @achievement.description %></p>
    <p><%= link_to @achievement.wowhead_url, @achievement.wowhead_url %></p>

    <%= render :partial => "shared/achievement_list", :object => @items %>
</div>